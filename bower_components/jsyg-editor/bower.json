{
    "name": "jsyg-editor",
    "authors" : ["Yannick Bochatay"],
    "description" : "Editor of svg chapes (JSYG framework)",
    "main": ["JSYG.Editor.js","JSYG.Editor.css"],
    "moduleType": [
        "amd",
        "globals"
    ],
    "dependencies": {
        "jsyg":"latest",
        "jsyg-selection":"latest",
        "jsyg-path":"latest",
        "jsyg-container":"latest",
        "jsyg-boundingbox":"latest",
        "jsyg-draggable":"latest",
        "jsyg-resizable":"latest",
        "jsyg-rotatable":"latest",
        "jsyg-alignment":"latest"
    },
    "devDependencies": {
        "qunit": "latest",
        "requirejs": "latest"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/YannickBochatay/JSYG.Editor.git"
    },
    "license": "MIT",
    "ignore": ["**/.*","tests","bower_components","nbproject"],
    "keywords": [
        "svg",
        "jsyg",
        "jquery",
        "javascript",
        "library"
    ]
